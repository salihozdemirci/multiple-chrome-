def basla8():
    if __name__ == '__main__':
        Thread(target = func1).start()
        Thread(target = func2).start()
        Thread(target = func3).start()
        Thread(target = func4).start()
        Thread(target = func5).start()
        Thread(target = func6).start()
        Thread(target = func7).start()
