def func1():
    driver1 = webdriver.Chrome(chrome_options=chrome_options)  
    driver1.set_window_position(0, 0)
    driver1.set_window_size(960, 540)
    driver1.get('https://www.google.com')
    



def func2():
    driver2 = webdriver.Chrome(chrome_options=chrome_options)
    driver2.set_window_position(960, 0)
    driver2.set_window_size(960, 540)
    driver2.get('https://www.google.com')
    
    
def func3():
